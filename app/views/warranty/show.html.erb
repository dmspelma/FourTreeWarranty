<h1>
  <%= @warranty.warranty_name %>
</h1>
<ul>
  <li>
    Company name: <%= @warranty.warranty_company %>
  </li>
  <li>
    Warranty Start Date: <%= @warranty.warranty_start_date %>
  </li>
  <li>
    Warranty End Date: <%= @warranty.warranty_end_date %>
  </li>
  <% if @warranty.extra_info %>
    <li>
      <%= "Extra Info: #{@Warranty.extra_info}" %>
    </li>
  <% end %>
</ul>

<%= link_to 'Home', warranty_index_path %>
<%= link_to 'Edit Warranty', edit_warranty_path %>
<%= link_to 'Delete', warranty_path(@warranty), data: { method: :delete, confirm: "Are you sure?" } %>
